<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="2024-12-01 12:30:00" author="avelizarov">
        <preConditions onSqlOutput="TEST"
                       onFailMessage="Таблица wallet уже существует">
            <not>
                <tableExists tableName="wallet"/>
            </not>
        </preConditions>
        <sqlFile path="create_table_wallet.sql"
                 relativeToChangelogFile="true"/>
        <rollback>
            <sqlFile path="rollback/create_table_wallet_rollback.sql"
                     relativeToChangelogFile="true"/>
        </rollback>
    </changeSet>

</databaseChangeLog>